{
  "name": "@splicemood/react-music-player",
  "homepage": "https://splicemood.github.io/react-music-player/",
  "description": "The music player that sync all settings between multiple tabs on single browser session locally",
  "version": "1.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splicemood/react-music-player"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "synchronization",
    "music-player",
    "playlist-manager",
    "react-hook",
    "react-provider",
    "multitab"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -Rf dist && tsc -p tsconfig.production.json",
    "publish": "npm publish --access public",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "react": "^18.x || ^19.x",
    "@mantine/hooks": "^7.14.2",
    "music-metadata": "^10.6.2"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "prettier": "^3.3.3"
  }
}
